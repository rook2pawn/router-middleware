{
  "name": "router-middleware",
  "version": "1.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "tsx --test ./tests/index.test.ts",
    "tsd": "npm run build && tsd",
    "watch": "run-p \"tsc -w\" \"node --watch --test dist",
    "test:cov": "c8 --hook-run-in-context --extension .ts --extension .tsx --include src/**/*.ts --exclude src/**/*.d.ts --exclude file://** --exclude-after-remap node --import=tsx --test tests/index.test.ts"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "c8": "^10.1.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "supertest-light": "^1.1.2",
    "tape": "^5.9.0",
    "tsd": "^0.31.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "tsd": {
    "directory": "test-d"
  }
}
