{
  "name": "router-middleware",
  "version": "6.0.3",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "npm run clean && npm run typecheck && tsup src/index.ts --format esm,cjs --dts --out-dir dist --clean --sourcemap",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "node --import tsx ./tests/all.tests.ts",
    "tsd": "npm run build && tsd",
    "watch": "run-p \"tsc -w\" \"node --watch --test dist",
    "test:cov": "c8 --hook-run-in-context --extension .ts --extension .tsx --include src/**/*.ts --exclude src/**/*.d.ts --exclude file://** --exclude-after-remap node --import=tsx --test tests/index.test.ts"
  },
  "devDependencies": {
    "@types/node": "^24.5.2",
    "c8": "^10.1.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "supertest-light": "^1.1.2",
    "tape": "^5.9.0",
    "ts-node": "^10.9.2",
    "tsd": "^0.31.0",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "tsd": {
    "directory": "test-d"
  }
}
